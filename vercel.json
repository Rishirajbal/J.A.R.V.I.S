{
  "version": 2,
  "builds": [
    { 
      "src": "backend/main.py", 
      "use": "@vercel/python" 
    },
    { 
      "src": "MCU/**", 
      "use": "@vercel/static" 
    }
  ],
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "backend/main.py" 
    },
    { 
      "src": "/(.*)", 
      "dest": "MCU/$1" 
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}